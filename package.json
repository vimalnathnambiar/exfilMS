{
  "name": "exfilms",
  "version": "0.0.0",
  "description": "A cross-platform, command line interface (CLI) tool to extract mass spectrometry (MS) data from mzML formatted files, with spectrum filtering capabilities.",
  "bin": {
    "exfilms": "./src/exfilms.js"
  },
  "type": "module",
  "scripts": {
    "eslint": "eslint src",
    "eslint-fix": "npm run eslint -- --fix",
    "prettier": "prettier --check src",
    "prettier-write": "prettier --write src",
    "vitest": "vitest --coverage",
    "vitest-ui": "vitest --coverage --ui",
    "test": "npm run vitest && npm run eslint && npm run prettier",
    "test-ui": "npm run vitest-ui && npm run eslint && npm run prettier"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vmalnathnambiar/exfilms.git"
  },
  "keywords": [
    "mass spectrometry",
    "data extraction",
    "spectrum filtering"
  ],
  "author": "Vimalnath Nambiar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vmalnathnambiar/exfilms/issues"
  },
  "homepage": "https://github.com/vmalnathnambiar/exfilms#readme",
  "devDependencies": {
    "@types/clear": "^0.1.4",
    "@types/figlet": "^1.5.8",
    "@types/inquirer": "^9.0.7",
    "@types/pako": "^2.0.3",
    "@types/papaparse": "^5.3.14",
    "@types/yargs": "^17.0.32",
    "@vitest/coverage-v8": "^1.3.1",
    "@vitest/ui": "^1.3.1",
    "eslint": "^8.57.0",
    "eslint-config-cheminfo": "^9.2.0",
    "prettier": "^2.8.8",
    "vitest": "^1.3.1"
  },
  "dependencies": {
    "arraybuffer-xml-parser": "^0.6.0",
    "base64-arraybuffer": "^1.0.2",
    "chalk": "^5.2.0",
    "clear": "^0.1.0",
    "figlet": "^1.6.0",
    "inquirer": "^9.2.7",
    "node-fetch": "^3.3.1",
    "ora": "^6.3.1",
    "pako": "^2.1.0",
    "papaparse": "^5.4.1",
    "yargs": "^17.7.2"
  }
}
